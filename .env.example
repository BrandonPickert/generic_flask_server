# CORS settings (comma-separated origins)
CORS_ORIGINS=*

# Database configuration (example)
# DATABASE_URL=postgresql://user:password@localhost/dbname

# Timezone
TIMEZONE=UTC

# Logging
LOG_LEVEL=INFO
